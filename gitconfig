	name = Jesper Skovgaard Nielsen

[alias]
	st = status
	ci = commit
	co = checkout
	b = branch
	aa = add --all
	head = log -1
	ds = diff --staged
	sb = "!git co `git b | selecta`"
	l = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%C(bold blue)<%an>%Creset' --abbrev-commit
	g = grep --break --heading --line-number
[push]
	default = current
[color]
	ui = true
